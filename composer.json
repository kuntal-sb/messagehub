{
    "name": "strivebenifits/messagehub",
    "description": "This is my package Messagehub",
    "keywords": [
        "StriveBenifits",
        "laravel",
        "messagehub"
    ],
    "homepage": "https://github.com/strivebenifits/messagehub",
    "license": "MIT",
    "authors": [
        {
            "name": "kuntal-strivebenefits",
            "email": "kuntal@strivebenefits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Strivebenifits\\Messagehub\\": "src",
            "Strivebenifits\\Messagehub\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strivebenifits\\Messagehub\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Strivebenifits\\Messagehub\\MessagehubServiceProvider"
            ],
            "aliases": {
                "Messagehub": "Strivebenifits\\Messagehub\\MessagehubFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
